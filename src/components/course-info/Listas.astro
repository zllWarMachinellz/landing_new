---
import type { Lista } from "@interfaces/courseInfo.interface";

interface Props {
    props: Lista | string;
}
const { props } = Astro.props;
// console.log(props)
---

<div class="text-white">
    <h4 class="text-2xl">{props.header}</h4>
    <ul class="list-disc">
        {props.body?.map(el => (<li>{el}</li>))}
    </ul>
</div>